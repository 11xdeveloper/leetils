{
  "name": "leetils",
  "version": "0.1.0",
  "description": "All (not really) of leetcode's problem solutions in one package.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/11xdeveloper/leetils.git"
  },
  "homepage": "https://github.com/11xdeveloper/leetils#readme",
  "bugs": {
    "url": "https://github.com/11xdeveloper/leetils/issues"
  },
  "keywords": [
    "javascript",
    "typescript",
    "utilities-library",
    "leetcode",
    "leetcode-solutions",
    "leetcode-javascript",
    "leetcode-typescript"
  ],
  "author": "11xdeveloper",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src --format cjs,esm --dts",
    "release": "changeset publish",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "check": "bun run format && bun run lint && bun test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "bun-types": "^1.0.15",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
