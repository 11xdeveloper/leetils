{
    "name": "leetils",
    "version": "0.0.3",
    "description": "All (not really) of leetcode's problem solutions in one package.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/11xdeveloper/leetils.git"
    },
    "homepage": "https://github.com/11xdeveloper/leetils#readme",
    "bugs": {
        "url": "https://github.com/11xdeveloper/leetils/issues"
    },
    "keywords": [
        "javascript",
        "typescript",
        "utilities-library",
        "leetcode",
        "leetcode-solutions",
        "leetcode-javascript",
        "leetcode-typescript"
    ],
    "author": "11xdeveloper",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src --format cjs,esm --dts",
        "release": "changeset publish",
        "test": "jest",
        "format": "prettier --write .",
        "lint": "eslint --fix .",
        "check": "bun run format && bun run lint && bun run test"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@types/jest": "^29.5.10",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "eslint": "^8.54.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.0",
        "ts-jest": "^29.1.1",
        "tsup": "^8.0.1",
        "typescript": "^5.3.2"
    }
}
